{
	"name": "Customer detail CSV to Table",
	"properties": {
		"activities": [
			{
				"name": "Copy CSV to table",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
